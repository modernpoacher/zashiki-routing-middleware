    "@babel/register": "^7.15.3",
    "babel-loader": "^8.2.2",
    "core-js": "^3.16.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "husky": "^7.0.2",
    "rimraf": "^3.0.2",
    "standard": "^16.0.3"
